{
  "name": "app_store_demo",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "clear": "pnpm dlx shx rm -rf dist",
    "lint": "eslint . --ext .vue,.ts",
    "lint:fix": "eslint . --ext .vue,.ts --fix",
    "build": "pnpm clear && vite build && pnpm copy && pnpm zip",
    "copy": "pnpm copy-config && pnpm copy-readme && pnpm copy-changelog",
    "copy-config": "pnpm dlx shx cp app-config.json ./dist/app-config.json",
    "copy-readme": "pnpm dlx shx cp README.md ./dist/README.md",
    "copy-changelog": "pnpm dlx shx cp CHANGELOG.md ./dist/CHANGELOG.md",
    "zip": "cd ./dist && pnpm dlx bestzip demo.zip * "
  },
  "dependencies": {
    "@antcde/component-library": "^0.1.2",
    "@antcde/connect-ts": "^0.4.1",
    "@antcde/vue-utils": "^0.2.0",
    "@mdi/font": "^7.4.47",
    "@thatopen/components": "~3.1.0",
    "@thatopen/components-front": "~3.1.0",
    "@thatopen/fragments": "~3.1.0",
    "@thatopen/ui": "~3.1.0",
    "@thatopen/ui-obc": "~3.1.0",
    "@vueuse/core": "^13.9.0",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.1",
    "vue-router": "^4.6.3",
    "vuetify": "^3.11.1",
    "@iconify-json/mdi": "^1.2.2",
    "@iconify/vue": "^5.0.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.0",
    "typescript": "^5.5.3",
    "unocss": "^66.5.9",
    "unplugin-icons": "^22.5.0",
    "vite": "^7.2.4",
    "vite-plugin-singlefile": "^2.3.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-tsc": "^3.1.5",
    "@types/node": "^24.10.1"
  },
  "private": true
}
